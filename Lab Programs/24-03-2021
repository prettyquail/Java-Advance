import java.util.*;
class Furniture{
	int id;
	String name;
	String desc;
    Furniture(int id,String name,String desc){
      this.id=id;
      this.name=name;
      this.desc=desc;
    }
    public String toString(){
        return (id + name + desc);
    }

	public static void main(String args[]){
	Furniture f1=new Furniture(01,"Chair","Well furnished Chair");
	Furniture f2=new Furniture(02,"Table","Well furnished Table");
	Furniture f3=new Furniture(03,"Sofa","Well furnished Sofa");
	Furniture f4=new Furniture(04,"Chair2","Well furnished Chair2");
	
	List<Furniture> list1=new LinkedList<>();
	List<Furniture> list2=new LinkedList<>(); 

	//add() method
  	list1.add(f1);  
  	list1.add(f2);  
  	list2.add(f3);
  	list2.add(f4);

  	//addAll() method
  	list1.addAll(list2);
    System.out.println(list1.size());

    //retainAll() method
    list1.retainAll(list2);
    for(Furniture f:list1){
    	System.out.println(f);
    }
  	
  	//retain() method
  	list2.remove(f3);
  	for(Furniture f:list2){
    	System.out.println(f);
    }

	}
}
